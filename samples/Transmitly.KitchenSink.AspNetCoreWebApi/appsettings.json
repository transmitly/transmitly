{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Transmitly": {
    "DefaultEmailFromAddress": "example@domain.com",
    "DefaultSmsFromAddress": "8881234567",
    "DefaultVoiceFromAddress": "8881234567",
    "ChannelProviders": {
      "Smtp": [
        {
          "IsEnabled": false,
          "Id": "Default",
          "Host": "smtp.example.com",
          "Username": "<username>",
          "Password": "<password>"
        }
      ],
      "Infobip": [
        {
          "IsEnabled": false,
          "Id": "Default",
          "ApiKey": "<apiKey>"
        }
      ],
      "Twilio": [
        {
          "IsEnabled": false,
          "Id": "Default",
          "AuthToken": "<authToken>",
          "AccountSid": "<accountSid>"
        }
      ],
      "Firebase": [
        {
          "IsEnabled": false,
          "Id": "Default"
        }
      ],
      "SendGrid": [
        {
          "IsEnabled": false,
          "ApiKey": "<apiKey>"
        }
      ]
    }
  }
}